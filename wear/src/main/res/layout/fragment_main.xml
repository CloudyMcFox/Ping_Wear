<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                tools:context="rageofachilles.textryan_wear.LaunchActivity">

    <android.support.wearable.view.WatchViewStub xmlns:android="http://schemas.android.com/apk/res/android"
                                                 xmlns:app="http://schemas.android.com/apk/res-auto"
                                                 xmlns:tools="http://schemas.android.com/tools"
                                                 android:id="@+id/watch_view_stub"
                                                 android:layout_width="match_parent"
                                                 android:layout_height="match_parent"
                                                 app:rectLayout="@layout/rect_activity_launch"
                                                 app:roundLayout="@layout/round_activity_launch"
                                                 tools:context="rageofachilles.textryan_wear.LaunchActivity"
                                                 tools:deviceIds="wear"></android.support.wearable.view.WatchViewStub>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PlaceHolder"
        android:id="@+id/lbl1"
        android:layout_marginTop="63dp"
        android:textSize="20dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:id="@+id/btnCancel"
        android:layout_below="@+id/lbl1"
        android:layout_centerHorizontal="true"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Resend"
        android:id="@+id/btnResend"
        android:layout_below="@+id/btnCancel"
        android:layout_centerHorizontal="true"/>


</RelativeLayout>
